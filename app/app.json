      "package": "com.felipemayer.robsolapp"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9d4cd834-a962-430e-a43e-812dca8b1c3d"
      }
    }
  }
